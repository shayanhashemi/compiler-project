fn find(A as vector, n as int) <int>
{
    k :: int = 0;
    j :: int;

    for (i = 0 to length(A))
    begin
        if [[n == k]]
        begin
            return j;
        end
        j = j + 1;
    end

    return -1;
}

fn main() <int>
{
    A :: vector;
    a :: int;

    A = list(3);
    A[0] = 1;
    A[1] = 2;
    A[2] = 3;

    print(find(A, a));
    print(find(A, 2));
    print(find(A, 3));

    return A;
}
